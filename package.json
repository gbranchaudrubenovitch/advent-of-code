{
  "name": "advent-of-code",
  "version": "1.0.0",
  "description": "daily challenges from adventofcode.com",
  "main": "main.mjs",
  "scripts": {
    "start": "node -r @std/esm main.mjs",
    "test": "mocha -r @std/esm --recursive --check-leaks --throw-deprecation --trace-warnings --use_strict ./test/**/*.*js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gbranchaudrubenovitch/advent-of-code.git"
  },
  "keywords": [
    "christmas"
  ],
  "author": "gbranchaudrubenovitch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gbranchaudrubenovitch/advent-of-code/issues"
  },
  "homepage": "https://github.com/gbranchaudrubenovitch/advent-of-code#readme",
  "jshintConfig": {
    "esnext": true,
    "node": true
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "mocha": "^3.5.0"
  },
  "dependencies": {
    "@std/esm": "^0.7.1",
    "graphlib": "^2.0.0",
    "js-combinatorics": "^0.5.0"
  }
}
